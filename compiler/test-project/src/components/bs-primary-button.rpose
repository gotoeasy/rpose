[api]
optionkeys = onclick, type, style
statekeys  = disabled
strict  = true

[view]
<button type="button" class="btn@bs btn-lg@bs dropdown-toggle@bs {
    btn-primary@bs: /primary/i.test(type)
    }"
    style={style}
    disabled={disabled}
    onclick={onclick}><slot/></button>

[options]
{
    type: 'primary'
}

[csslib]
bs = bootstrap:**.min.css

