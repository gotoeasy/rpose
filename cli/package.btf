[name] // 项目名 （必填）
@rpose/cli

[version] // 版本号 （必填）
0.2.2



[main] // 入口文件
index.js

[bin] // cli命令
rpose = bin/rpose

[dependencies] // 依赖库
@gotoeasy/rpose = *
commander = *
chalk = *
inquirer = *

[devDependencies] // 开发依赖库

[description] // 描述
rpose命令行工具

[keywords] // 检索关键字，多个关键字用半角逗号分隔
gotoeasy, rpose, rpose-cli

[scripts] // 脚本命令
test = echo "Error: no test specified" && exit 1

[repository] // 仓库
type= git
url = git+https://github.com/gotoeasy/rpose.git

[homepage] // 主页，github项目会自动识别设定
https://github.com/gotoeasy/rpose/blob/master/cli/README.md

[bugs] // BUG提交地址，github项目会自动识别设定
url = https://github.com/gotoeasy/rpose/issues

[author] // 作者信息
name = 青松
email= gotoeasy@163.com

[license] // 协议
Apache-2.0

